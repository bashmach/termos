{
    "name": "Termos",
    "version": "0.0.32",
    "manifest_version": 2,
    "description": "Disclosure of pitfalls of the legal documents of the web",
    "icons": {
        "16": "images/unknown-19.png",
        "128": "images/unknown-38.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "scripts/background.js",
            "scripts/livereload.js",
            "scripts/vendor.js"
        ]
    },
    "browser_action": {
        "default_icon": {
            "19": "images/unknown-19.png",
            "38": "images/unknown-38.png"
        },
        "default_title": "termos",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "background",
        "webRequest",
        "webRequestBlocking",
        "http://*/*",
        "https://*/*"
    ]
}